{{> header}}

<div class="profile col-md-6 text-center col-lg">
    <img class="profile-img" src="./images/user.png"/>
    <div class="profile-info">
        <p>Username: <small>{{userInfo.username}}</small></p>
        <p class="infoType">Wished {{wished}} treks =)</p>
        {{#each treksInfo}}
            <p>{{this.location}}</p>
        {{else}}
            <p>No treks</p>
        {{/each}}
    </div>
</div>

{{> footer}}